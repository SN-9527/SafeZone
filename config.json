{
  "log": {
    "level": "warn"
  },
  "dns": {
    "servers": [
      {
        "address": "tls://8.8.8.8",
        "detour": "proxy"
      }
    ],
    "strategy": "ipv4_only",
    "disable_cache": false,
    "disable_expire": false
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "address": [
        "172.19.0.1/30"
      ],
      "mtu": 1500,
      "auto_route": true,
      "strict_route": true,
      "stack": "gvisor"
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "vless",
      "server": "104.16.0.0",
      "server_port": 8443,
      "uuid": "9dd1cfbe-f4e4-46ce-800b-c24bfdb5c790",
      "tls": {
        "enabled": true,
        "server_name": "www.nireus.cc"
      },
      "transport": {
        "type": "ws",
        "path": "/greatfirewall",
        "headers": {
          "Host": "www.nireus.cc"
        }
      }
    },
    {
      "tag": "direct",
      "type": "direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "domain_keyword": [
          "baidu",
          "tencent",
          "alipay",
          "taobao",
          "meituan",
          "qq.com",
          "weixin",
          "wechat",
          "douyin",
          "bilibili",
          "pinduoduo",
          "yangkeduo",
          "alibaba",
          "xiaohongshu"
        ],
        "domain_suffix": [
          ".cn",
          "jd.com",
          "apple.com",
          "icloud.com"
        ],
        "outbound": "direct"
      }
    ]
  }
}