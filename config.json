{
  "log": {
    "level": "warn"
  },
  "dns": {
    "rules": [
      {
        "domain_suffix": [
          ".cn",
          "qq.com"
        ],
        "server": "alibaba"
      },
      {
        "domain_suffix": [
          "apple.com",
          "icloud.com"
        ],
        "server": "alibaba"
      },
      {
        "rule_set": [
          "geosite-cn"
        ],
        "server": "alibaba"
      }
    ],
    "servers": [
      {
        "tag": "google",
        "address": "tls://8.8.8.8",
        "detour": "proxy"
      },
      {
        "tag": "alibaba",
        "address": "tls://223.5.5.5",
        "detour": "direct"
      }
    ],
    "independent_cache": true,
    "strategy": "ipv4_only",
    "disable_cache": false,
    "disable_expire": false
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "address": [
        "172.19.0.1/30"
      ],
      "mtu": 1500,
      "auto_route": true,
      "strict_route": true,
      "stack": "gvisor"
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "vless",
      "server": "104.16.0.0",
      "server_port": 8443,
      "uuid": "9dd1cfbe-f4e4-46ce-800b-c24bfdb5c790",
      "tls": {
        "enabled": true,
        "server_name": "www.nireus.cc"
      },
      "transport": {
        "type": "ws",
        "path": "/greatfirewall",
        "headers": {
          "Host": "www.nireus.cc"
        }
      }
    },
    {
      "tag": "direct",
      "type": "direct"
    }
  ],
  "route": {
    "rule_set": [
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/SagerNet/sing-geosite/raw/refs/heads/rule-set/geosite-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-ads",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/SagerNet/sing-geosite/raw/refs/heads/rule-set/geosite-category-ads.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/SagerNet/sing-geoip/raw/refs/heads/rule-set/geoip-cn.srs",
        "download_detour": "proxy"
      }
    ],
    "auto_detect_interface": true,
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "rule_set": [
          "geosite-ads"
        ],
        "action": "reject"
      },
      {
        "domain_suffix": [
          "apple.com",
          "icloud.com"
        ],
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geosite-cn"
        ],
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geoip-cn"
        ],
        "outbound": "direct"
      }
    ]
  }
}